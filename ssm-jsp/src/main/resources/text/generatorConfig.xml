<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
    <classPathEntry location="F:\TSB_ISCHOOL2_EXTERNAL_API\WebRoot\WEB-INF\lib\mysql-connector-java-5.1.26-bin.jar"/>
    <context id="context1">
        <!-- 去掉注释 -->
        <commentGenerator>
            <property name="suppressAllComments" value="true"/>
            <property name="suppressDate" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://127.0.0.1:3306/seckill?useUnicode=true&amp;characterEncoding=utf-8&amp;zeroDateTimeBehavior=convertToNull&amp;transformedBitIsBoolean=true"
                        userId="root" password="root"/>
        <!-- 禁止类型强制转换 -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>
        <!-- 生成bean类存放位置 -->
        <javaModelGenerator targetPackage="org.seckill.entity"
                            targetProject="src/main/java">
        </javaModelGenerator>
        <!-- 生成mapper文件存放位置 -->
        <sqlMapGenerator targetPackage="org.seckill.entity"
                         targetProject="src/main/java">
        </sqlMapGenerator>
        <!-- 生成Dao类存放位置 -->
        <javaClientGenerator targetPackage="org.seckill.entity"
                             type="XMLMAPPER" targetProject="src/main/java">
        </javaClientGenerator>

        <table tableName="seckill" domainObjectName="SuccessKilledBean22"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false">
            <!-- 设定是否使用驼峰生成规则 -->
            <property name="useActualColumnNames" value="false"/>
        </table>


    </context>
</generatorConfiguration>
